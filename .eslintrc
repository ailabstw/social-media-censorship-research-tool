{
  "env": {
    "browser": true,
    "mocha": true
  },
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": 0,
    "import/prefer-default-export": 0,

    "no-underscore-dangle": 0,
    "yoda": 0,
    "prefer-rest-params": 0
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
